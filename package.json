{
  "name": "dblp",
  "version": "1.4.1",
  "packageManager": "pnpm@9.15.4",
  "description": "An awesome DBLP search application",
  "author": "sabertazimi",
  "license": "MIT",
  "homepage": "https://github.com/sabertazimi/dblp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/dblp.git"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/dblp/issues"
  },
  "keywords": [
    "dblp",
    "computer science",
    "react"
  ],
  "main": "./src/index.js",
  "engines": {
    "node": ">=9"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "dev": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "format": "prettier --write src/**/*.{js,jsx,css}",
    "lint": "stylelint ./src/**/*.css && eslint ./src",
    "lint:fix": "stylelint ./src/**/*.css --fix && eslint ./src --fix",
    "changeset": "commit-and-tag-version --dry-run -s",
    "release": "commit-and-tag-version -s",
    "start": "pnpm dev"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "antd": "^5.23.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@dg-scripts/eslint-config": "^5.21.5",
    "@dg-scripts/stylelint-config": "^5.21.5",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "commit-and-tag-version": "^12.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.18.0",
    "eslint-webpack-plugin": "^4.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "style-loader": "^4.0.0",
    "stylelint": "^16.13.2",
    "stylelint-webpack-plugin": "^5.0.1",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
