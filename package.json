{
  "name": "dblp",
  "version": "1.4.1",
  "packageManager": "pnpm@10.26.1",
  "description": "An awesome DBLP search application",
  "author": "sabertazimi",
  "license": "MIT",
  "homepage": "https://github.com/sabertazimi/dblp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/dblp.git"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/dblp/issues"
  },
  "keywords": [
    "dblp",
    "computer science",
    "react"
  ],
  "main": "./src/index.js",
  "engines": {
    "node": ">=9"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "dev": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "format": "prettier --write src/**/*.{js,jsx,css}",
    "lint": "stylelint ./src/**/*.css && eslint ./src",
    "lint:fix": "stylelint ./src/**/*.css --fix && eslint ./src --fix",
    "changeset": "commit-and-tag-version --dry-run -s",
    "release": "commit-and-tag-version -s",
    "start": "pnpm dev"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "antd": "^6.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-object-rest-spread": "^7.28.4",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@dg-scripts/eslint-config": "^6.0.0",
    "@dg-scripts/stylelint-config": "^6.0.0",
    "autoprefixer": "^10.4.23",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "commit-and-tag-version": "^12.6.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-webpack-plugin": "^5.0.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.5",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.7.4",
    "style-loader": "^4.0.0",
    "stylelint": "^16.26.1",
    "stylelint-webpack-plugin": "^5.0.1",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^5.1.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
